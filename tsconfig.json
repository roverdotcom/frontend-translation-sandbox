{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es2015",
        "allowJs": true,
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
          "@rover/*": ["src/*"]
        }
    },
    "include": [
        "./src/**/*"
    ]
}
